{
    "name": "wanze/seo-maestro",
    "description": "Seo Maestro allows to easily manage your page's meta data and sitemap.",
    "type": "processwire-module",
    "extra": {
        "installer-name": "SeoMaestro"
    },
    "keywords": ["processwire", "seo", "metatags", "sitemap"],
    "homepage": "https://github.com/wanze/SeoMaestro",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Wanzenried",
            "email": "stefan.wanzenried@gmail.com",
            "homepage": "https://github.com/wanze"
        }
    ],
    "require": {
        "php": ">=7.0",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6"
    },
    "autoload-dev": {
        "psr-4": {
            "SeoMaestro\\Test\\": "tests/src/"
        }
    }
}
